{
  "name": "demo-js-debugging",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "run-p clean lint",
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build:scripts": "rollup --config",
    "clean": "npx rimraf dist/*",
    "dev": "run-p watch:*",
    "lint": "eslint .",
    "server": "cross-env NODE_ENV=production node server/app.js",
    "watch": "run-p watch:*",
    "watch:scripts": "npm run build:scripts -- --watch",
    "watch:server": "nodemon --watch server --inspect server/app.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "cross-env": "7.0.0",
    "dynamic-import-polyfill": "0.1.1",
    "eslint": "6.8.0",
    "eslint-config-vi": "10.8.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "rollup": "1.31.1",
    "rollup-plugin-terser": "5.2.0"
  },
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.18.2",
    "http-errors": "1.7.3",
    "node-fetch": "2.6.0",
    "pug": "2.0.4",
    "serve-favicon": "2.5.0"
  }
}
